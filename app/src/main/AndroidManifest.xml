<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.budfamily">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity_result"></activity>
        <activity android:name=".activity_settings" />
        <activity android:name=".activity_add_cost" />
        <activity android:name=".activity_add_bill" />
        <activity android:name=".activity_choose_pay" />
        <activity android:name=".activity_choose_change" />
        <activity android:name=".activity_choose_add" />
        <activity
            android:name=".activity_choose"
            android:label="@string/title_activity_choose" />
        <activity android:name=".activity_first_start" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>